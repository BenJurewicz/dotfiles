        {
            "complex_modifications": {
                "parameters": { "basic.to_if_alone_timeout_milliseconds": 200 },
                "rules": [
                    {
                        "description": "Caps Lock → '`' when pressed alone, Ctrl when held with other keys, Ctrl+Caps Lock for actual Caps Lock",
                        "manipulators": [
                            {
                                "description": "Ctrl+Caps Lock → Caps Lock",
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": {
                                        "mandatory": ["control"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "key_code": "caps_lock" }],
                                "type": "basic"
                            },
                            {
                                "description": "Caps Lock → Ctrl (or '`' if pressed alone)",
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "to_if_alone": [{ "key_code": "grave_accent_and_tilde" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Double ctr to fn or Raycast Emoji Menu if alone",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "name": "control pressed",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "fn" }],
