                            },
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "control pressed",
                                            "value": 1
                                        }
                                    },
                                    { "key_code": "left_control" }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "control pressed",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
